Full pages go in here, see below for an example:

import React, { Component } from "react";
import Card from "../components/utils/card";
import newComponent from "../components/newComponent";
import "../style.css";

class Login extends Component { //extends component means you can access react functions
  
  handleInputChange = event => { //react function to handle when a text box changes
    
  };

  render() {
    return (
      <div id="profile">
        <Card title="Login" insert={<newComponent/>} />
      </div>
    );
  };
};

export default Login;