This is where helper components are placed, they can be used to construct templates to insert information

import React from "react";
import "./style.css";

function Card(props) { //props are the parameters passed to the function
  return (
    <div className="container"> //container is a bootstrap class
      <div className="card"> //card is a bootstrap class
        <h2>
          {props.title}
        </h2>
        <div className="card-body">
          {props.insert}
        </div>
      </div>
    </div>
  );
};

export default Card;